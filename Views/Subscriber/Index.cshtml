@model IEnumerable<Subscriber>
@{
    ViewData["Title"] = "Index";
}

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Subscriber Home Page!</h1>
@if(TempData["keyMessage"]!=null)
{
    <h2>@TempData["keyMessage"]</h2>
}

<a class="nav-link text-dark" asp-area="" asp-controller="Subscriber" asp-action="Create">Add a new Subscriber</a>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Subscription Level</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="40%">@obj.Name</td>
                <td width="40%">@obj.subLevel</td>
                <td width="40%">@obj.age</td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Subscriber" asp-action="Edit" asp-route-id="@obj.Id"><span class="material-symbols-outlined">settings</span></a></td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Subscriber" asp-action="Delete" asp-route-id="@obj.Id"><span class="material-symbols-outlined">delete</span></a></td>
            </tr>
        }
    </tbody>
</table>
